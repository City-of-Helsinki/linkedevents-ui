    "eslint-loader": "^2.1.2",
    "express": "^4.13.3",
    "git-revision-webpack-plugin": "^2.4.0",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.4",
    "intl": "^1.0.1",
    "isomorphic-fetch": "^2.2.0",
    "jest": "23.6.0",
    "jquery": "^3.4.0",
    "jszip": "^3.0.0",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.13",
    "loose-envify": "^1.4.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "nconf": "^0.9.1",
    "node-sass": "^4.14.1",
    "object-assign": "^4.1.1",
    "oidc-client": "^1.10.1",
    "progress": "^1.1.8",
    "prop-types": "^15.7.2",
    "pug": "^2.0.4",
    "raven-js": "^2.3.0",
