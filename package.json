    "react-leaflet": "^2.7.0",
    "react-loader": "^2.0.0",
    "react-redux": "^4.4.10",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^3.0.8",
    "react-transform-hmr": "^1.0.4",
    "reactstrap": "^8.4.1",
    "redux": "4.0.5",
